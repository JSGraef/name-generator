{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "member-access": [
      true,
      "no-public"
    ],
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "member-access": [
      true,
      "no-public"
    ],
    "jsx-no-lambda": false,
    "no-console": false
  },
  "jsRules": {
    "no-console": false
  }
}